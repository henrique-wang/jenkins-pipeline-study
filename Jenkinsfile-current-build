@Library('shared-library') _

pipeline {
  agent any
  stages {
    stage('Hello') {
      steps {
        sh 'echo Hello World'
        echo "Build number is ${currentBuild.number}"
        helloWorld(name: 'Admin tester', dayOfWeek: 'Monday')
      }
    }
  }
  post {
    always {
      echo currentBuild.currentResult
    }
  }
}